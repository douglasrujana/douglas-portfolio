{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".astro/dist/client"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/_astro/.*",
      "headers": { "cache-control": "public, max-age=31536000, immutable" }
    },
    {
      "src": "/assets/.*",
      "headers": { "cache-control": "public, max-age=31536000, immutable" }
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": ".astro/dist/client"
}
